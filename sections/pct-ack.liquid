#circle {
    width: 10vh;
    height: 10vh;
    border-radius: 50%;
    background-position: center;
    background-size: cover;
    position: relative;
    overflow: hidden;
    transition: 0.3s;
    border: solid 3px #000;
    box-shadow: 0 0 6px 6px #fff;
}
#circle:hover {
    border: solid 3px #fff;
    box-shadow: 0 0 6px 6px #b1b1b1;
    transition: 0.3s;
}
#overlay{
  display:none;
  position:absolute;
  bottom:0;
  text-align:center;
  width:10vh;
  height:10vh; 
  background-color:rgba(255,255,255,0.5);
  transition: 0.3s;
}
#circle:hover #overlay{
