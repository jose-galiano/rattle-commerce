<a class="docu_link" target="_blank" href="{{ 'products.SOL.flyer_link' | t }}">{{ 'products.product.flyer_tag' | t }}</a>
<a class="docu_link" target="_blank" href="{{ 'products.SOL.brochure_link' | t }}">{{ 'products.product.brocure_tag' | t }}</a>
