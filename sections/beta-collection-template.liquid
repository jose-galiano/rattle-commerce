{%include 'include-TMS-Collection'%}
<style>
iframe {
    display: none;
}</style>

<div class="detail-sections">
 

{% if section.blocks.size > 0 %}
  

  {% for block in section.blocks %}
    <div {{ block.shopify_attributes }} class="block__{{ block.type }} block__{{ block.id }}{% if block.type == "image_with_text_overlay" and block.settings.wide_display %} under-menu{% elsif block.type == "featured_video" and block.settings.wide_display %} under-menu{% elsif block.type == "featured_collection" %} featured-collection-section{% endif %}"id="{{block.type}}_{{utm_code}}">
      {% if block.type == "blog_posts" and block.settings.blog_posts_UTM == utm_code %}
        <!-- Blog posts -->
        {% include 'include-featured-blog', type: 'block' %}

      {% elsif block.type == "contact_form" and block.settings.contact_form_UTM == utm_code %}
        <!-- Contact form -->
        {% include 'include-contact-form', type: 'block' %}

      {% elsif block.type == "featured_video" and block.settings.featured_video_UTM == utm_code %}
        <!-- Featured video -->
        {% include 'include-video', type: 'block' %}

       {% elsif block.type == 'image_gallery' and block.settings.image_gallery_UTM == utm_code %}
        <!-- Image gallery -->
        {% include 'include-gallery', type: 'block' %}
      
       {% elsif block.type == 'gallery_cols' and block.settings.image_gallery_cols_UTM == utm_code %}
        <!-- Gallery Columns-->
        {% include 'include-gallery-cols', type: 'block' %}
      
      {% elsif block.type == 'image_gallery_txt' and block.settings.image_gallery_txt_UTM == utm_code %}
        <!-- Image gallery Txt -->
        {% include 'include-gallery-txt', type: 'block' %}
      
      {% elsif block.type == 'image_gallery_feats' and block.settings.image_gallery_feats_UTM == utm_code %}
        <!-- Image gallery -->
        {% include 'include-gallery-feat', type: 'block' %}

      {% elsif block.type == "image_with_text"  and block.settings.image_with_text_UTM == utm_code %}
        <!-- Image with text -->
        {% if block.settings.layout == false %}
          <div class="container standard--width">
        {% endif %}
            

            
        <div id="{{block.settings.section_id}}" class="image-with-text
          featured_collections
          clearfix across-1 image-align--{{ block.settings.frontpage_image_position }}">
          <section class="featured-link--section featured-link--image section-{{ forloop.index }}">
            <div class="featured-link--half featured-link--image {% if block.settings.layout == false %}featured-link--margin{% endif %} image-crop--{{ block.settings.image_crop }}">
              <div class="featured-link--wrap">
                <a data-no-instant {% if block.settings.link != blank %}href="{{ block.settings.link }}"{% endif %} title="{{ block.settings.title }}">
                  {% if block.settings.image %}
                  
                    {% include 'image-element', image: block.settings.image.src, alt: block.settings.image.alt %}
                  {% else %}
                    {% capture i %}{% cycle "1", "2", "3", "4", "5", "6" %}{% endcapture %}
                    {{ 'collection-' | append: i | placeholder_svg_tag: 'placeholder-svg placeholder-svg--promotions' }}
                  {% endif %}
                </a>
              </div>
            </div>

            <div class="featured-link--half">
              <div class="info text-align--{{block.settings.frontpage_text_align}}">
                <a data-no-instant {% if block.settings.link != blank %}href="{{ block.settings.link }}"{% endif %} class="collection_title">
                  {{ block.settings.title }}
                  {{ block.settings.image_feat.src}}
                </a>
                {% if block.settings.text != blank %}
                  <div class="description">{{ block.settings.text }}</div>
                {% endif %}
                {% if block.settings.button_label != blank %}
                  <a data-no-instant {% if block.settings.link != blank %}href="{{ block.settings.link }}"{% endif %} class="button">
                    {{ block.settings.button_label }}
                  </a>
                {% endif %}
              </div>
            </div>
          </section>
        </div>
        {% if block.settings.layout == false %}
          </div>
        {% endif %}

        {% if block.settings.layout == false %}
          <style>
            #shopify-section-{{ section.id }} .featured-link--section {
              margin-bottom: 3%;
            }
            @media only screen and (max-width: 480px) {
              #shopify-section-{{ section.id }} .featured-link--section {
                margin-bottom: 10%;
              }
            }

            .standard--width .featured-link--margin {
              margin-left: 0%;
              margin-right: 3%;
            }

            .standard--width .image-align--right .featured-link--margin {
              margin-left: 3%;
              margin-right: 0%;
            }

            .standard--width .featured-link--half {
              width: 45%;
            }

      {% else %}
            <style>
                       
            .featured-link--half {
  		     background-color: {{block.settings.half_bg_color}} !important;
		    }
            .description p{
             font-size: {{block.settings.desc_f_s}} !important;
             color:{{block.settings.desc_f_c}} !important;
            } 
            
            .description a{
             color:{{block.settings.link_c}} !important;
            } 
            .description a:hover{
             color:{{block.settings.link_h_c}} !important;
            }

          </style>
        {% endif %}

      {% elsif block.type == 'map' and block.settings.map_UTM == utm_code %}
        <!-- Map -->
        {% include 'include-map', type: 'block' %}

      {% elsif block.type == "page" and block.settings.page_UTM == utm_code %}
        <!-- Page -->
        {% include 'include-content-page', type: 'block' %}

      {% elsif block.type == 'quote' and block.settings.quote_UTM == utm_code %}
        <!-- Quote -->
        {% include 'include-quote', type: 'block' %}
      
      {% elsif block.type == 'responsive_text_and_image' and block.settings.responsive_text_and_image_UTM == utm_code %}
        <!-- Responsive text and image -->
        {% include 'include-responsive-featured-text', type: 'block' %}

      {% elsif block.type == "rich_text_and_image" and block.settings.rich_text_and_image == utm_code %}
        <!-- Rich text and image -->
        {% include 'include-featured-text', type: 'block' %}

      {% elsif block.type == 'divider' %}
        <!-- Divider -->
        {% include 'include-divider', type: 'block' %}

      {% elsif block.type == "featured_collection" and block.settings.featured_collection_UTM == utm_code %}
        <!-- Featured collection -->
        {% include 'include-featured-collection', type: 'block' %}

      {% elsif block.type == "image_with_text_overlay" and block.settings.image_with_text_overlay_UTM == utm_code%}
        <!-- Image with text overlay -->
        {% include 'include-image-with-text-overlay', type: 'block' %}

      {% elsif block.type == "recently_viewed_products" and block.settings.recently_viewed_products_UTM == utm_code%}
        <!-- Related Products -->
        {% include 'include-recently-viewed', type: 'block' %}

      {% elsif block.type == "custom_html" and block.settings.custom_html_UTM == utm_code%}
        <!-- Custom HTML -->
        {% include 'include-custom-html', type: 'block' %}

       {% elsif block.type == "comparison_table" and block.settings.custom_html_UTM == utm_code %}
        <!-- comparison_table -->
        {% include 'tms-compare-table', type: 'block'  %}
      
      {% elsif block.type == "comparison_table_2" and block.settings.custom_html_UTM == utm_code %}
        <!-- comparison_table 2x10 -->
        {% include 'tms-compare-table-2', type: 'block'  %}
      
       {% elsif block.type == "header_landing" and block.settings.header_landing_UTM == utm_code %}
        <!-- header_landing -->
        {% include 'header_landing', type: 'block'  %}
      
      {% elsif block.type == "standalone_title" and block.settings.standalone_title_UTM == utm_code %}
        <!-- Standalone Title -->
        {% include 'standalone_title', type: 'block'  %}
      
      
      
        {% elsif block.type == "UTM_Setup" %}
          {% assign utm_1 = block.settings.block_utm_cat_1 %}
          {% assign utm_2 = block.settings.block_utm_cat_2 %}
          {% assign utm_3 = block.settings.block_utm_cat_3 %}
        		
            {% if handle == utm_1 %}
              {% assign utm_code = 'utm1' %}
              {%  elsif handle == utm_2 %}
              {% assign utm_code = 'utm2' %}
              {%  elsif handle == utm_3 %}
              {% assign utm_code = 'utm3' %}
              {% else %}
              {% assign utm_code = 'gen' %} 
              {% endif %}
            {% endif %}
      
      
      
    </div>

  {% endfor %}
{% endif %}

