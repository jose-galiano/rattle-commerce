{% if customer.tags contains 'wholesale' %}

	{% assign dealer = "true"%}
              {% if customer.tags contains 'bronze' %}
                  {% assign dealer_level = 'bronze'%}
                  {% assign dealer_badge = '#cd7f32'%}
              		{%else%}
                    {% if customer.tags contains 'platinum' %}
                        {% assign dealer_level = 'platinum'%}
                        {% assign dealer_badge = '#bab7b2'%}
                            {%else%}
                                {% if customer.tags contains 'gold' %}
                                {% assign dealer_level = 'gold'%}
                                {% assign dealer_badge = '#FFD700'%}
                                  {%else%}
                                      {% if customer.tags contains 'silver' %}
                                      {% assign dealer_level = 'silver'%}
                                      {% assign dealer_badge = '#A9A9A9'%}
                                      {% endif %}
                                 {% endif %}
                    {% endif %}
                {% endif %}

{% else %}

{% assign dealer = "false"%}

{% endif %}

<a name="pagecontent" id="pagecontent"></a>
<section id="account">
<div class="product-details__blocks">
   
    {% if section.blocks.size > 0 %}
      {% for block in section.blocks %}

        <div {{ block.shopify_attributes }} class="block__{{ block.type }} block__{{ block.id }} {% if block.type == "featured_collection" %}featured-collection-section{% endif %}">
  
          {% if block.type == "blog_posts" %}
            <!-- Blog posts -->
            {% include 'include-featured-blog', type: 'block' %}

          {% elsif block.type == "header_landing"%}
        <!-- header_landing -->
        {% include 'header_landing', type: 'block'  %}
          
          {% elsif block.type == "faq"%}
        <!-- faq wholesale -->
        {% include 'include-faq-wholesale', type: 'block'  %}
          
          {% elsif block.type == "customer_details"%}
        <!-- customer details -->
        {% include 'include-customer', type: 'block'  %}
          
          {% elsif block.type == "customer_support_ticket"%}
        <!-- customer details -->
        {% include 'customer-support-ticket', type: 'block'  %}
          
          {% elsif block.type == "contact_form"%}
            <!-- Contact form -->
            {% include 'include-contact-form', type: 'block' %}

          {% elsif block.type == "featured_video" %}
            <!-- Featured video -->
            {% include 'include-video', type: 'block' %}
          
           {% elsif block.type == 'gallery_cols'%}
       		 <!-- Gallery Columns-->
           {% include 'include-gallery-cols', type: 'block' %}

          {% elsif block.type == 'image_gallery' %}
            <!-- Image gallery -->
            {% include 'include-gallery', type: 'block' %}

          {% elsif block.type == "image_with_text"%}
            <!-- Image with text -->
            {% include 'include-image-with-text', type: 'block' %}
          
          {% elsif block.type == 'image_gallery_txt'%}
          <!-- Image gallery Txt -->
          {% include 'include-gallery-txt', type: 'block' %}

          {% elsif block.type == 'map' %}
            <!-- Map -->
            {% include 'include-map', type: 'block' %}

          {% elsif block.type == "page" %}
            <!-- Page -->
            {% include 'include-content-page', type: 'block' %}

          {% elsif block.type == 'quote'%}
            <!-- Quote -->
            {% include 'include-quote', type: 'block' %}
          
          {% elsif block.type == 'testimonial'%}
            <!-- Testimonial -->
            {% include 'include-testimonial', type: 'block' %}

          {% elsif block.type == "rich_text_and_image" %}
            <!-- Rich text and image -->
            {% include 'include-featured-text', type: 'block' %}

          {% elsif block.type == 'product_reviews' %}
            <!-- Reviews -->
          {% include 'include-reviews', type: 'block' %}
           
          {% elsif block.type == 'divider' %}
            <!-- Divider -->
            {% include 'include-divider', type: 'block' %}

          {% elsif block.type == "featured_collection" %}
            <!-- Featured collection -->
            {% include 'include-featured-collection', type: 'block' %}

          {% elsif block.type == "image_with_text_overlay" %}
            <!-- Image with text overlay -->
            {% include 'include-image-with-text-overlay', type: 'block' %}

          {% elsif block.type == "recently_viewed_products"%}
            <!-- Recently Viewed Products -->
            {% include 'include-recently-viewed', type: 'block' %}

          {% elsif block.type == "related_products"%}
            <!-- Related Products -->
            {% include 'include-related-products', type: 'block' %}

          {% elsif block.type == "custom_html" %}
            <!--Custom HTML -->
            {% include 'include-custom-html', type: 'block' %}
          
          {% elsif block.type == "scroll_view"%}
            <!--scroll_view -->
            {% include 'include-scroll_view', type: 'block' %}
          
          {% elsif block.type == "comparison_table_2"%}
        <!-- comparison_table 2x10 -->
        {% include 'tms-compare-table-2', type: 'block'  %}
          
          {% elsif block.type == "standalone_title" %}
        <!-- Standalone Title -->
          {% include 'standalone_title', type: 'block'  %} 
 		 
          {% elsif block.type == "chart" %}
        <!-- Chart -->
          {% include 'include-chart', type: 'block'  %} 
          
            {% elsif block.type == "Schedule" %}
        <!-- schedule -->
          {% include 'include-schedule', type: 'block'  %} 
          
          {% elsif block.type == "cost_schedules" %}
        <!-- schedule -->
          {% include 'include-cost-schedules', type: 'block'  %} 
          

        </div>
  
  {% endif %}
	
      {% endfor %}
    {% endif %}
  </div>
</section>

