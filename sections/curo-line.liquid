        type="button"
        data-mdb-toggle="collapse"
        data-mdb-target="#flush-collapse-{{ resin_name | t | downcase | split: " " | last }}"
        aria-expanded="false"
        aria-controls="flush-collapse-{{ resin_name | t | downcase | split: " " | last }}"
